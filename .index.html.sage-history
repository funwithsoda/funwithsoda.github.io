nt | split:\"<!-- more -->\" | first % }}\n    {% else %}\n      {{ post.content }}\n    {% endif %}\n  </div><!-- /.entry-content -->\n</article><!-- /.hentry -->\n{% endfor %}\n\n<div class=\"pagination\">\n  {% if paginator.previous_page %}\n    {% if paginator.previous_page == 1 %}\n      <a href=\"{{ site.url }}\" class=\"btn\">Previous</a>\n    {% else %}\n      <a href=\"{{ site.url }}/page{{ paginator.previous_page }}\" class=\"btn\">Previous</a>\n    {% endif %}\n  {% else %}\n    Previous\n  {% endif %}\n  <ul class=\"inline-list\">\n    <li>\n      {% if paginator.page == 1 %}\n        <span class=\"current-page\">1</span>\n      {% else %}\n        <a href=\"{{ site.url }}\">1</a>\n      {% endif %}\n    </li>\n    {% for count in (2..paginator.total_pages) %}\n      <li>\n        {% if count == paginator.page %}\n          <span class=\"current-page\">{{ count }}</span>\n        {% else %}\n          <a href=\"{{ site.url }}/page{{ count }}\">{{ count }}</a>\n        {% endif %}\n      </li>\n    {% endfor %}\n  </ul>\n  {% if paginator.next_page %}\n    <a href=\"{{ site.url }}/page{{ paginator.next_page }}\" class=\"btn\">Next</a>\n  {% else %}\n    Next\n  {% endif %}\n</div><!-- /.pagination -->"],[1,"test"],[0,"\n"]],0,0,3048,5]],"time":1453070535018}